DEFINE CLASS form_regn AS form 
 	*< CLASSDATA: Baseclass="form" Timestamp="" Scale="" Uniqueid="" />

	*-- OBJECTDATA items order determines ZOrder / El orden de los items OBJECTDATA determina el ZOrder 
	*< OBJECTDATA: ObjPath="Text1" UniqueID="" Timestamp="" />
	*< OBJECTDATA: ObjPath="Command1" UniqueID="" Timestamp="" />

	AlwaysOnTop = .T.
	Caption = "Поиск по рег.№ "
	DoCreate = .T.
	Height = 18.13
	Left = 0.00
	Name = "FORM_Regn"
	ScaleMode = 0
	Top = 0.00
	Width = 85.00

	ADD OBJECT 'Command1' AS commandbutton WITH ;
		Caption = "ОК", ;
		Height = 1.80, ;
		Left = 33.40, ;
		Name = "Command1", ;
		Top = 8.80, ;
		Width = 13.00
		*< END OBJECT: BaseClass="commandbutton" />

	ADD OBJECT 'Text1' AS textbox WITH ;
		FontSize = 10, ;
		Format = "T", ;
		Height = 1.53, ;
		Left = 24.00, ;
		Name = "Text1", ;
		Top = 4.00, ;
		Width = 36.00
		*< END OBJECT: BaseClass="textbox" />
	
	PROCEDURE Destroy
		On Key Label F5
		Pop Key
		
	ENDPROC

	PROCEDURE Init
		With This
			.Left = 17/7*(Sysmetric(1)/100)
			.Top  = 6/19.5*(Sysmetric(2)/100)
		Endwith
		Push Key Clear
		
		On Key Label F5 Do act_poisk4
		
		
	ENDPROC

	PROCEDURE Command1.Click
		With Thisform
			Select (al)
			trec = Recno()
			Set Order To Tag Regn
			Seek .Text1.Value
		
			If !Found()
				Messagebox('Рег.№ НЕ НАЙДЕН!',0+48,'Внимание!',2000)
				Go trec
				.Text1.SetFocus
			Else
				.Release
			Endif
		Endwith
		
	ENDPROC

	PROCEDURE Text1.MouseDown
		Lparameters nButton, nShift, nXCoord, nYCoord
		x_q_my=nXCoord
		y_q_my=nYCoord
		If nButton=2
			Do vs_menu
		Endif
		
	ENDPROC

ENDDEFINE
