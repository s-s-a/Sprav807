DEFINE CLASS w_com_d AS form 
 	*< CLASSDATA: Baseclass="form" Timestamp="" Scale="" Uniqueid="" />

	*-- OBJECTDATA items order determines ZOrder / El orden de los items OBJECTDATA determina el ZOrder 
	*< OBJECTDATA: ObjPath="Label1" UniqueID="" Timestamp="" />
	*< OBJECTDATA: ObjPath="Text1" UniqueID="" Timestamp="" />
	*< OBJECTDATA: ObjPath="Command1" UniqueID="" Timestamp="" />

	AlwaysOnTop = .T.
	Caption = "Сравнение с датой"
	DoCreate = .T.
	Height = 271
	Left = 0
	Name = "w_com_d"
	Top = 0
	Width = 468

	ADD OBJECT 'Command1' AS commandbutton WITH ;
		Caption = "OK", ;
		Height = 27, ;
		Left = 170, ;
		Name = "Command1", ;
		Top = 167, ;
		Width = 84
		*< END OBJECT: BaseClass="commandbutton" />

	ADD OBJECT 'Label1' AS label WITH ;
		AutoSize = .T., ;
		Caption = "Дата:", ;
		FontSize = 13, ;
		Height = 22, ;
		Left = 119, ;
		Name = "Label1", ;
		Top = 92, ;
		Width = 45
		*< END OBJECT: BaseClass="label" />

	ADD OBJECT 'Text1' AS textbox WITH ;
		Alignment = 3, ;
		FontSize = 13, ;
		Height = 30, ;
		Left = 184, ;
		Name = "Text1", ;
		Top = 89, ;
		Value = {}, ;
		Width = 116
		*< END OBJECT: BaseClass="textbox" />
	
	PROCEDURE Init
		With This
			.Left = 30*(Sysmetric(1)/100)
			.Top  = 6*(Sysmetric(2)/100)
		Endwith
		
	ENDPROC

	PROCEDURE Command1.Click
		With Thisform
			If .Text1.Value > Date()
				Messagebox('Сравнить с датой больше системной невозможно!',0+16,'Некорректная дата',3000)
				Return
			Endif
		*ssa*	qqo=pathdata+'a807'+SUBSTR(DTOC(ThisForm.Text1.Value),7,4)+SUBSTR(DTOC(ThisForm.Text1.Value),4,2)+SUBSTR(DTOC(ThisForm.Text1.Value),1,2)+'.dbf'
		*ssa*	qqk=pathdata+'a807'+SUBSTR(DTOC(ThisForm.Text1.Value),7,4)+SUBSTR(DTOC(ThisForm.Text1.Value),4,2)+SUBSTR(DTOC(ThisForm.Text1.Value),1,2)+'.cdx'
		*!*		qqo=pathdata+'a807'+Dtos(.Text1.Value)+'.dbf'
		*!*		qqk=pathdata+'a807'+Dtos(.Text1.Value)+'.cdx'
		
			If (!(File(pathdata+'a807'+Dtos(.Text1.Value)+'.dbf'))) &&.OR.(!(FILE(qqk)))
				Messagebox('Отсутстует DBF-таблица БИК-ов (или индексный файл)!'+Chr(13)+;
					'Необходимо сначала скачать справочник за указанную дату и сохранить по F7 !',0+48,'Внимание!',8000)
				Return
			Endif
		
			If .Text1.Value = fr_start.Text1.Value
				Messagebox('Сравнивать две одинаковые даты не нужно!',0+16,'Некорректная дата',3000)
				Return
			Endif
		
			=frtfcompare(.T., "Data\lstcompare.RTF")
			.Release
		Endwith
		
		* LOCAL loF as Wscript.Shell
		* ff1=pathdata+'compdat.txt'
		* prms = 'notepad.exe'+' '+ff1
		* loF=CREATEOBJECT("WScript.Shell")
		* loF.Run(prms, 1, .F.) && .F. не ждать выполнения notepad.exe
		* Release loF
		
		*-----------------------------
		
	ENDPROC

ENDDEFINE
